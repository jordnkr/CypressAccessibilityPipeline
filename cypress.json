{
    "baseUrl": "http://localhost:8080",
    "video": false,
    "reporter": "./node_modules/cypress-multi-reporters",
    "reporterOptions": {
        "configFile": "./reporter.json"
    },
    "env": {
        "A11Y_OPTIONS": {
            "runOnly": {
                "type": "tag",
                "values": ["wcag2a", "wcag2aa"]
            }
        },
        "urls": {
            "index": "/index.html",
            "pagetwo": "/pagetwo.html"
        }
    }
}
